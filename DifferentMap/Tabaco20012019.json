{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Tabacoo Impact on Australia",
    "width": 700,
    "height": 600,
    "projection": {"type": "equirectangular"},
    "data": {
      "url": "https://raw.githubusercontent.com/stha0035/Assingment2-2977275/main/AustralianStates%20(6).topojson",
      "format": {"type": "topojson", "feature": "AustralianStates"}
    },
    "transform": [
        {
        "lookup": "properties.STATE_NAME",
        "from": {
        "data": {
        "url": "https://raw.githubusercontent.com/stha0035/Assingment2-2977275/main/UpdatedTabacoo2001-2019.csv"
        },
            "key": "State",
            "fields": ["Smoking status"]
        }
        }
        ],
        "mark": {"type": "geoshape"},
        "encoding": {
            "color": {
                "field": "2001",
                "type": "quantitative",
                "scale": {"type": "log"}
        },
        "tooltip": [
            {"field": "properties.STATE_NAME", "type": "nominal", "title": "Country"},
            {"field": "Active", "type": "quantitative"}
        ]
        }
  }

  